<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef>
    <defName>Ears_Orassan</defName>
    <label>orassan ears</label>
    <description>Carriers of this gene have orassan-like ears.</description>
    <iconPath>UI/Icons/Genes/Gene_EarCat</iconPath>
    <biostatCpx>0</biostatCpx>
    <endogeneCategory>Ears</endogeneCategory>
    <displayCategory>Cosmetic</displayCategory>
    <displayOrderInCategory>70</displayOrderInCategory>
    <iconColor>(0.75, 0.75, 0.75)</iconColor>
	<randomChosen>true</randomChosen><!--
    <graphicData>
      <layer>PostHair</layer>
      <drawNorthAfterHair>true</drawNorthAfterHair>
      <useSkinShader>true</useSkinShader>
      <drawLoc>HeadTop</drawLoc>
      <graphicPath>Things/Pawn/Humanlike/HeadAttachments/CatEars/CatEars</graphicPath>
      <colorType>Skin</colorType>
    </graphicData>-->
    <exclusionTags>
      <li>Ears</li>
    </exclusionTags>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>cat</symbol></li>
        <li><symbol>kitty</symbol></li>
        <li><symbol>kit</symbol></li>
        <li><symbol>tabby</symbol></li>
        <li><symbol>tom</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>cat</symbol></li>
        <li><symbol>kitten</symbol></li>
        <li><symbol>tabby</symbol></li>
        <li><symbol>tom</symbol></li>
        <li><symbol>ear</symbol></li>
      </suffixSymbols>
    </symbolPack>
  </GeneDef>

  <GeneDef>
    <defName>Tail_Orassan</defName>
    <label>orassan tail</label>
    <description>Carriers of this gene grow a slender but furry tail that can act as a dexterous fifth limb.</description>
    <iconPath>Things/Pawn/Humanlike/OrassanTails/OTail_north</iconPath>
    <iconColor>(0.75, 0.75, 0.75)</iconColor>
    <displayOrderInCategory>1000</displayOrderInCategory>
    <displayCategory>Miscellaneous</displayCategory>
    <exclusionTags>
      <li>Tail</li>
    </exclusionTags>
	<biostatCpx>0</biostatCpx>
	<randomChosen>true</randomChosen><!--
    <graphicData>
      <drawLoc>Tailbone</drawLoc>
    </graphicData>
    <capMods>
      <li>
        <capacity>Manipulation</capacity>
        <offset>0.05</offset>
      </li>
    </capMods>
    <biostatCpx>1</biostatCpx>
    <biostatMet>-1</biostatMet>
    <graphicData>
      <graphicPath>Things/Pawn/Humanlike/BodyAttachments/SmoothTail/SmoothTail</graphicPath>
      <colorType>Hair</colorType>
      <drawOffsetNorth>(0, 0.2, -0.15)</drawOffsetNorth>
      <drawOffsetSouth>(0, -0.1, -0.15)</drawOffsetSouth>
      <drawOffsetEast>(-0.5, -0.1, -0.15)</drawOffsetEast>
    </graphicData>-->
  </GeneDef>

